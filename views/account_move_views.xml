<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_account_gt_move_form" model="ir.ui.view">
            <field name="name">account_gt.account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <field name="fiscal_position_id" position="after">
                    <field name="liquidacion_id"/>
                    <field name="tipo_factura"/>
                </field>
                <page name="other_info" position="after">
                    <page name="datos_fexp" string="Datos Factura exportaciÃ³n">
                        <group>
                            <field name="nombre_consignatario_destinatario"/>
                            <field name="direccion_consignatario_destinatario"/>
                            <field name="codigo_consignatario_destinatario"/>
                            <field name="nombre_comprador"/>
                            <field name="direccion_comprador"/>
                        </group>   
                        <group>
                            <field name="codigo_comprador"/>
                            <field name="otra_referencia"/>
                            <field name="incoterm_exp"/>
                            <field name="nombre_exportador"/>
                            <field name="codigo_exportador"/>
                        </group>  
                    </page>    
                </page>
            </field>
        </record>
        
    <record id="infilefel_view_company_form" model="ir.ui.view">
      <field name="name">infilefel.res.company.form</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form"/>
      <field name="arch" type="xml">
        <field name="vat" position="after">
            <field name="anulado_libro_compras"/>
        </field>    
      </field>
    </record>  
        
    </data>
</odoo>
