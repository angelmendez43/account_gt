      <field name="report_file">account_gt.liquidacion</field>
      <field name="binding_model_id" ref="account_gt.model_account_gt_liquidacion"/>
      <field name="binding_type">report</field>
  </record>

  <record id="action_libro_conciliacion_bancaria" model="ir.actions.report">
      <field name="name">Libro conciliacion bancaria</field>
      <field name="model">account_gt.libro_conciliacion_bancaria.wizard</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">account_gt.reporte_libro_conciliacion_bancaria</field>
      <field name="report_file">account_gt.reporte_libro_conciliacion_bancaria</field>
      <field name="binding_model_id" ref="account_gt.model_account_gt_libro_conciliacion_bancaria_wizard"/>
      <field name="binding_type">report</field>
  </record>

